#include <iostream>
#include <math.h>
using namespace std;
int main()
{
   float x1,x2,y1,y2,d;
    cout<<"Ingrese la coordenada y del primer punto,x1:"<<endl;
    cin>>x1;
    cout<<"Ingresar la  coordenada y del primer punto,y1:"<<endl;
    cin>>y1;
    cout<<"Ingresar la  coordenada x del segundo punto,x2:"<<endl;
    cin>>x2;
    cout<<"Ingresar la  coordenada y del segundo punto,y2:"<<endl;
    cin>>y2;
    d=sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    cout<<"La distancia entre los dos puntos es:"<<d;
    return 0;
}
